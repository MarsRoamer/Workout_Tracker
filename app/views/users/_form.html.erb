<div class="form-group">
        <%= f.label :name, "Name:", class: "col-xs-2 control-label" %>
        <div class="col-xs-10 trial">
          <%= f.text_field :name, class: "form-control" %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :city, "City:", class: "col-xs-2 control-label" %>
        <div class="col-xs-10 trial">
          <%= f.text_field :city, class: "form-control" %>
        </div>
      </div>

      <div class="form-group">
        <%= f.label :state, "State:", class: "col-xs-2 control-label" %>
        <div class="col-xs-10 trial">
          <%= f.text_field :state, class: "form-control" %>
        </div>
      </div>

       <div class="form-group">
         <label class="col-xs-2 control-label">Gym Affiliation: </label>
        <div class="col-xs-10 trial">
          <%=f.collection_select(:gym_id, Gym.all[0...-1], :id, :name, {:include_blank => 'Select your gym'}, class: "form-control") %>
        </div>
      </div>

      <div class="form-group">
         <label class="col-xs-2 control-label">Email: </label>
        <div class="col-xs-10 trial">
          <%= f.text_field :email, class: "form-control" %>
        </div>
      </div>

      <div class="form-group">
         <label class="col-xs-2 control-label">Username: </label>
        <div class="col-xs-10 trial">
          <%= f.text_field :username, class: "form-control" %>
        </div>
      </div>

      <div class="form-group">
         <label class="col-xs-2 control-label">Password: </label>
        <div class="col-xs-10 trial">
          <%= f.password_field :password, class: "form-control" %>
        </div>
      </div>
      


