 <nav class="navbar navbar-default">
  <div class="container">
    <div class="navbar-header">
      <a href="/"  class="navbar-brand">WOD Tracker</a>
    </div>
    <ul class="nav navbar-nav navbar-right">
      <li><a href="/owners/<%=@owner.id%>"><%=@owner.username%>'s Profile</a></li>
      <li><a href="/logout">Logout</a></li>
    </ul>
   </div>
 </nav>

<% if @owner.errors.any? %>
  <div id="error_explanation">
    <h2>There were some errors:</h2>
    <ul>
      <% @owner.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
    </ul>
  </div>
<% end %>

<%= form_for @owner, :html => {:class => "form-horizontal"} do |f| %>
	<%= render :partial => "form", :locals=>{:f=>f} %>
	<%=f.submit "Edit Profile" %><br><br>
<%end%>