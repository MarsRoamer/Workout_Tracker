<%=form_for [@owner, @gym, @workout], url: {action: "create"} do |f| %>
	<label>Name: </label>
	<%=f.text_field :name %>
	<label>Conditioning type:</label>
	<%=f.text_field :conditioning_type %>
	<label>Description: </label>
	<%=f.text_area :description %>
	<%=f.submit "Create Workout" %>
<%end%>